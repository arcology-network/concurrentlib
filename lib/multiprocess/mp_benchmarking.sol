pragma solidity >=0.8.0 <0.9.0;

import "./Multiprocess.sol";
import "../array/Bool.sol";
import "../commutative/U256Cum.sol";

contract MpBenchmarking {
    /**
     * @notice Reverses the input bytes array.
     * @param message The bytes array to be reversed.
     */
    function reverse(bytes memory message) public pure {
        bytes memory reversed = new bytes(message.length);
        for(uint i=0; i<message.length; i++){
            reversed[ message.length - i - 1] = message[i];
        }
        keccak256(reversed); 
    }

    function benchmarkReverseString10k() public {
        string memory message = "";
        bytes memory funcCall = abi.encodeWithSignature("reverse(bytes)", address(this), message);
        Multiprocess mp = new Multiprocess(32);
        for (int i = 0; i < 10000; i ++) { 
             mp.push(500000, address(this), funcCall);
        }
        mp.run();
    }
}